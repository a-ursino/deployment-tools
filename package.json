{
  "name": "deployment-tools",
  "version": "1.0.6",
  "description": "A node.js scripts that helps you to compile and deploy the static assets(css/JavaScript) of your website without gulp and grunt using node and npm script",
  "main": "index.js",
  "author": "Alessandro Ursino (killanaca)",
  "engines": {
    "node": ">=6.0.0"
  },
  "config": {
    "domain": "http://az889637.vo.msecnd.net",
    "projectName": "projectname",
    "webConfig": "/data/Web.config",
    "packageJson": "/data/package.json",
    "srcJsPath": "/data/scripts/",
    "mainJs": "main.js",
    "mainBackoffileJs": "main-backoffice.js",
    "buildPathJs": "/data/bundles/",
    "srcSassOUT": "/sass/",
    "srcLess": "/data/less/",
    "mainStyle": "main.less",
    "mainBackoffileStyle": "main-admin.less",
    "buildPathCss": "/data/css/",
    "preserveBuildPathCss": true,
    "pre-git": {
      "commit-msg": "conventional",
      "pre-commit": [
        "npm run lint"
      ],
      "pre-push": [],
      "post-merge": []
    }
  },
  "configDev": {
  },
  "scripts": {
    "USE THIS SCRIPTS TO SIMULATE THE USE IN A REAL PROJECT": "###########",
    "testDev": "babel-node node_modules/blue-tape/bin/blue-tape ./test/**/*.js | faucet",
    "lintDev": "cross-env DEBUG=dt DEV=true babel-node src/run lint",
    "cleanDev": "cross-env DEBUG=dt DEV=true babel-node src/run clean",
    "buildDev": "cross-env DEBUG=dt DEV=true babel-node src/run build",
    "bumpDev": "cross-env DEBUG=dt DEV=true babel-node src/run bump",
    "deployDev": "cross-env DEBUG=dt DEV=true babel-node src/run deploy",
    "uploadDev": "cross-env DEBUG=dt DEV=true babel-node src/run upload",
    "watchDev": "cross-env DEBUG=dt DEV=true babel-node src/run watch",
    "GENERAL PACKAGE SCRIPTS": "########",
    "publish-please": "publish-please",
    "prepublish": "publish-please guard",
    "postinstall": "node dist/run postinstall",
    "test": "babel-node node_modules/blue-tape/bin/blue-tape ./test/**/*.js | faucet",
    "changeloginit": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 500",
    "doc": "esdoc -c ./esdoc.json",
    "build": "babel src -d dist",
    "lint": "cross-env DEBUG=dt DEV=true babel-node src/run lint"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "debug": "^2.2.0",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "pre-git": "^3.8.4",
    "publish-please": "^2.1.4",
    "sinon": "^1.17.4",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "azure-storage": "^1.0.1",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-trailing-function-commas": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "blue-tape": "^0.2.0",
    "bluebird": "^3.4.0",
    "chokidar": "^1.5.1",
    "cross-env": "^1.0.8",
    "cssnano": "^3.6.2",
    "del": "^2.2.0",
    "dotenv": "^2.0.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "faucet": "0.0.1",
    "imports-loader": "^0.6.5",
    "less": "^2.7.1",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss": "^5.0.21",
    "progress-bar-webpack-plugin": "^1.8.0",
    "recursive-readdir": "^2.0.0",
    "semver": "^5.1.0",
    "tape": "^4.5.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
